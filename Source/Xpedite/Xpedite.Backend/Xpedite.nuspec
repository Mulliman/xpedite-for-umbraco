<?xml version="1.0"?>
<package>
	<metadata>
		<id>XPEDITE</id>
		<version>15.0.0-alpha1</version>
		<authors>Sam Mullins</authors>
		<owners>Sam Mullins</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This is a code generator built for Umbraco so that you can create headless components quickly.</description>
		<readme>README.md</readme>
		<title>XPEDITE</title>
		<targetFramework>net9.0</targetFramework>
		<dependencies>
			<dependency id="Umbraco.Cms" version="[15.2.0,)" />
		</dependencies>
	</metadata>

	<files>
		<!-- Include compiled DLLs -->
		<file src="bin\Release\net9.0\*.dll" target="lib/net9.0" />

		<!-- Include frontend files -->
		<file src="ui\**\*.*" target="contentFiles/any/any/ui/" />

		<!-- Include the targets file -->
		<file src="build\nuget.targets" target="build\nuget.targets" />
	</files>
</package>
